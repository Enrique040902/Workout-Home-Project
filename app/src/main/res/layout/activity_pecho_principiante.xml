<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".PechoPrincipianteActivity">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:fontFamily="casual"
        android:text="Pecho principiante"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Perdida aprox. de 27.9 Kcal"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#858484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="16dp"
                android:padding="4dp">


                <ImageView
                    android:id="@+id/imgeSaltoTijera"
                    android:layout_width="99dp"
                    android:layout_height="99dp"
                    app:srcCompat="@drawable/saltos_de_tijera" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginBottom="28dp"
                    android:layout_toEndOf="@+id/imgeSaltoTijera"
                    android:text="Saltos de tijera"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textColor="@color/black" />

                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/saltos_tijera_rutina"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView9"
                    android:layout_marginTop="-20dp"
                    android:layout_toEndOf="@+id/imgeSaltoTijera" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="99dp"
                    android:layout_height="99dp"
                    app:srcCompat="@drawable/flexiones_inclinacion_apoyo_brazos" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="160dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginBottom="28dp"
                    android:layout_toEndOf="@+id/imageView2"
                    android:text="Flexiones con inclinación (Apoyo de brazos)"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textColor="@color/black" />

                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/FlexionesInclinacion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-20dp"
                    android:layout_below="@id/textView10"
                    android:layout_toEndOf="@+id/imageView2"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="16dp">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="99dp"
                    android:layout_height="99dp"
                    app:srcCompat="@drawable/flexiones_simples" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginBottom="28dp"
                    android:layout_toEndOf="@+id/imageView3"
                    android:text="Flexiones"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textColor="@color/black" />

                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/FlexionesNormales"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-20dp"
                    android:layout_below="@id/textView11"
                    android:layout_toEndOf="@+id/imageView3"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="16dp">

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="99dp"
                    android:layout_height="99dp"
                    app:srcCompat="@drawable/principiante_flexion_brazos_abiertos" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginBottom="28dp"
                    android:layout_toEndOf="@+id/imageView4"
                    android:text="Flexiones con brazos abiertos"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textColor="@color/black" />

                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/FlexionesAbiertas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-20dp"
                    android:layout_below="@id/textView12"
                    android:layout_toEndOf="@+id/imageView4"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="99dp"
                    android:layout_height="99dp"
                    app:srcCompat="@drawable/principiante_flexiones_triceps_con_silla" />

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginBottom="28dp"
                    android:layout_toEndOf="@+id/imageView6"
                    android:text="Flexiones con tríceps en silla"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textColor="@color/black" />

                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/FlexionesTricepSilla"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-20dp"
                    android:layout_below="@id/textView14"
                    android:layout_toEndOf="@+id/imageView6"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="16dp">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="99dp"
                    android:layout_height="99dp"
                    app:srcCompat="@drawable/flexiones_inclinacion_apoyo_brazos" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="160dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginBottom="28dp"
                    android:layout_toEndOf="@+id/imageView5"
                    android:text="Flexiones con inclinación (Apoyo de brazos)"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textColor="@color/black" />

                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/FlexionesInclinacion2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-20dp"
                    android:layout_below="@id/textView13"
                    android:layout_toEndOf="@+id/imageView5"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="16dp">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="99dp"
                    android:layout_height="99dp"
                    app:srcCompat="@drawable/principiante_flexion_brazos_abiertos" />

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginBottom="28dp"
                    android:layout_toEndOf="@+id/imageView7"
                    android:text="Flexiones con brazos abiertos"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textColor="@color/black" />

                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/FlexionesAbiertas2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-20dp"
                    android:layout_below="@id/textView15"
                    android:layout_toEndOf="@+id/imageView7"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="16dp">

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="99dp"
                    android:layout_height="99dp"
                    app:srcCompat="@drawable/principiante_flexiones_triceps_con_silla" />

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginBottom="28dp"
                    android:layout_toEndOf="@+id/imageView8"
                    android:text="Flexiones con tríceps en silla"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textColor="@color/black" />

                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/FlexionesTricepSilla2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-20dp"
                    android:layout_below="@id/textView16"
                    android:layout_toEndOf="@+id/imageView8"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp">

                <ImageView
                    android:id="@+id/imageView16"
                    android:layout_width="99dp"
                    android:layout_height="99dp"
                    app:srcCompat="@drawable/principiante_flexiones_de_brazos_con_rodillas_apoyadas" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginBottom="28dp"
                    android:layout_toEndOf="@+id/imageView16"
                    android:text="Flexiones con apoyo de rodillas"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textColor="@color/black" />

                <androidx.compose.ui.platform.ComposeView
                    android:id="@+id/FlexionesApoyoRodilla"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-20dp"
                    android:layout_below="@id/textView17"
                    android:layout_toEndOf="@+id/imageView16"/>

            </RelativeLayout>


        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>